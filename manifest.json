{
  "manifest_version": 2,
  "name": "Gif Searcher 4000",
  "version": "1.5",
  "description": "A Unique Way to Spend Time on the Internet!",
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./main.js"],
      "css": ["./main.css"]
    }
  ],
  "browser_action": {
      "default_icon": "./yoda.png",
      "default_popup": "./index.html",
      "default_title": "Search for a GIF"
  }
}


