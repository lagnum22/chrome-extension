{
  "manifest_version": 2,
  "name": "Character Search",
  "version": "1.5",
  "description": "It helps user brush up on their pop culture knowledge",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./main.js"],
      "css": ["./main.css"]
    }
  ],
  "browser_action": {
      "default_icon": "./yoda.png",
      "default_popup": "./index.html",
      "default_title": "Search for a GIF"
  }
}


